<div class="flex min-h-screen">
            <!-- Left side of flex container -->
            <div
                class="container flex-none w-3/12 mx-auto bg-gray-100 border border-gray-300 shadow-sm"
            >
                <!-- This is the container for the items in the left main container -->
                <div class="flex flex-col mt-8 mb-8">
                    <div class="flex justify-between m-4">
                        <h1
                            class="font-semibold tracking-wide text-gray-700 text-md"
                        >
                            Projects
                        </h1>
                        <button
                            class="px-3 py-1 font-mono text-sm text-gray-200 bg-green-700 rounded-md shadow-md hover:bg-green-800"
                            type="submit"
                        >
                            <i class="fas fa-book"></i> New
                        </button>
                    </div>
                    <!-- This will be the list of items-->
                    <div class="relative flex flex-col">
                        <!-- projec list -->
                        <div class="pb-6 mt-5 ml-6">
                            <ul class="text-gray-600 list-disc list-inside"></ul>

                            {{#each projects}}
                                <li><a href="{{this.directory}}">{{this.title}}</a></li>
                               {{/each}}
                            </ul>
                        </div>
                        <!-- About me  -->
                        <div class="mt-3">
                            <h1
                                class="block mt-3 font-semibold tracking-wide text-center text-gray-700 border-t border-gray"
                            >
                                About me
                            </h1>
                            <p
                                class="px-3 mt-3 text-sm text-center text-gray-600"
                            >
                                Lorem ipsum dolor sit amet consectetur,
                                adipisicing elit. Cum accusamus quae distinctio
                                placeat eos? Reprehenderit vitae corrupti,
                                suscipit, vel, laborum qui eveniet inventore
                                unde nihil eligendi sapiente aliquid vero
                                mollitia.
                            </p>
                        </div>
                    </div>
                    <div
                        class="absolute ml-3 text-sm italic font-thin text-gray-600 uppercase bottom-1"
                    >
                        MIT License <i class="fas fa-copyright"></i> 2021
                    </div>
                </div>
            </div>
            <!-- Right side of flex container -->
            <div class="flex-auto rounded-sm bg-grey-200">
                <div class="flex flex-col m-auto mt-24 bg-red-200 w-max">
                    <div><a href="{{link}}">
                        <img
                            class="rounded-lg shadow-lg"
                            src="{{link}}/screenshot.jpg"
                            alt=""
                        /></a>
                    </div>
                    <div><h1>{{title}}</h1></div>
                    <div>
                        <p class=""></p>
                    </div>
                </div>
            </div>
        </div>